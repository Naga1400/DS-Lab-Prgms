#include <stdio.h>
#include <stdlib.h>
#define Max 5

typedef struct Stack{
int arr[Max];
int top;
}Stack;

void initializeStack(Stack *s){
 s->top = -1;
}

int isEmpty(Stack *s){
return s->top == -1;
}

int isFull(Stack *s){
return s->top == Max -1;
}

int push(Stack *s,int value){
if (isFull(s)){
    printf("Stack Overflow occured",value);
    return s->top=0;
}
s->arr[++(s->top)]=value;
printf("Inserted Item %d\n:",value);
}

int pop(Stack *s){
if (isEmpty(s)){
    printf("Stack Underflow occured");
    return -1;
}                               
s->arr[(s->top)--];
}

void display(Stack *s){
    if (isEmpty(s)){
        printf("Stack is Empty");
        return;
    }
    printf("Stack elements are :");
    for(int i=0;i <= s->top;i++){
        printf("%d\n",s->arr[i]);
    }
    printf("\n");
}

int main(){
Stack s;
initializeStack(&s);

push(&s,10);
push(&s,20);
push(&s,30);

display(&s);

printf("Popped Item %d",pop(&s));
display(&s);
return 0;
}
